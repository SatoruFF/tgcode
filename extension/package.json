{
  "name": "tgcode",
  "displayName": "tgcode",
  "description": "VSCode + telegram",
  "version": "1.0.3",
  "publisher": "SatoruFujinuma",
  "icon": "assets/icon.png",
  "categories": [
    "Other"
  ],
  "files": [
    "src/**",
    "binaries/**",
    "assets/**",
    "package.json",
    "README.md"
  ],
  "engines": {
    "vscode": "^1.91.0"
  },
  "activationEvents": [],
  "main": "./src/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/SatoruFF/tgcode.git"
  },
  "contributes": {
    "commands": [
      {
        "command": "telegram-vscode.openTelegramWeb",
        "title": "Open Telegram in Tab"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "telegram-sidebar",
          "title": "Telegram",
          "icon": "assets/sidebar_icon.svg"
        }
      ]
    },
    "views": {
      "telegram-sidebar": [
        {
          "type": "webview",
          "id": "telegramWebview",
          "name": "Telegram"
        }
      ]
    }
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
